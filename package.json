{
  "name": "revery",
  "version": "0.32.0",
  "description": "App toolkit built with Reason",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bryphe/revery/issues"
  },
  "scripts": {
    "test": "esy b dune runtest",
    "format": "esy #{os == 'windows' ? 'b' : ''} bash .ci/format.sh #{os}",
    "run": "esy @examples x Examples"
  },
  "homepage": "https://github.com/bryphe/revery#readme",
  "esy": {
    "buildEnv": {
      "PKG_CONFIG_PATH": "/usr/lib64/pkgconfig:$PKG_CONFIG_PATH"
    },
    "exportedEnv": {
      "PKG_CONFIG_PATH": {
        "val": "/usr/lib64/pkgconfig:$PKG_CONFIG_PATH",
        "scope": "global"
      }
    },
    "build": "dune build -p reason-harfbuzz,reason-skia,reason-sdl2,timber,rebez,flex,Revery",
    "install": [
      "esy-installer reason-harfbuzz.install",
      "esy-installer reason-skia.install",
      "esy-installer reason-sdl2.install",
      "esy-installer Revery.install",
      "bash -c \"#{os == 'windows' ? 'cp /usr/x86_64-w64-mingw32/sys-root/mingw/bin/*.dll \\'$cur__bin\\'': ':'}\"",
      "bash -c \"cp #{esy-mono-skia.bin}/skia.dll \\'$cur__bin\\' #{os == 'windows' ? '' : '2>/dev/null || true'}\"",
      "bash -c \"cp #{esy-sdl2.bin}/*.dll \\'$cur__bin\\' #{os == 'windows' ? '' : '2>/dev/null || true'}\"",
      "bash -c \"cp #{esy-angle-prebuilt.bin}/*.dll \\'$cur__bin\\' #{os == 'windows' ? '' : '2>/dev/null || true'}\""
    ]
  },
  "dependencies": {
    "@opam/bos": "*",
    "@opam/ctypes": "0.23.0",
    "@opam/dune": "< 4.0.0",
    "@opam/dune-configurator": "*",
    "@opam/reason": "^3.6.2",
    "@opam/lru": "*",
    "@opam/lwt": "^5.9.1",
    "@opam/lwt_ppx": "^5.9.1",
    "@opam/markup": "1.0.3",
    "@opam/ppx_deriving": "*",
    "@opam/ppx_optcomp": "v0.16.0",
    "@opam/omd": "ocaml/omd:omd.opam#1535e3c",
    "@opam/uucp": "*",
    "@opam/uutf": "*",
    "@opam/charInfo_width": "*",
    "@brisk/brisk-reconciler": "briskml/brisk-reconciler#c9d5c4c",
    "esy-angle-prebuilt": "1.0.0",
    "esy-freetype2": "^2.9.1008",
    "@onivim/reason-native-crash-utils": "^1.0.2",
    "esy-harfbuzz": "syaiful6/esy-harfbuzz#8c653146d5abea4c6ca4045450bc8bee7e45f232",
    "esy-mono-skia": "syaiful6/esy-mono-skia#ed44f9368b8c9185c2d74a6edbd717a1f8dc0ef2",
    "esy-sdl2": "syaiful6/esy-sdl2#600ed68d14a985afe78551e6a2f2109d1cf9af0b",
    "reperf": "syaiful6/reperf#27f9e34747f1b1c5ff7efdfc28452315f29f26b1",
    "rench": "^1.10.0",
    "@reason-native/rely": "*",
    "ocaml": "4.14.x"
  },
  "resolutions": {
    "@opam/printbox": "0.12",
    "@opam/stdio": "v0.16.0",
    "@reason-native/rely": "git+https://github.com/reasonml/reason-native.git:rely.opam#20b1997b6451d9715dfdbeec86a9d274c7430ed8",
    "@opam/cli": "git+https://github.com/reasonml/reason-native.git:cli.opam#20b1997b6451d9715dfdbeec86a9d274c7430ed8",
    "@opam/pastel": "git+https://github.com/reasonml/reason-native.git:pastel.opam#20b1997b6451d9715dfdbeec86a9d274c7430ed8",
    "@opam/file-context-printer": "git+https://github.com/reasonml/reason-native.git:file-context-printer.opam#20b1997b6451d9715dfdbeec86a9d274c7430ed8"
  },
  "devDependencies": {
    "esy-astyle": "zbaylin/esy-astyle#59bc21a",
    "@opam/merlin": "*",
    "@opam/ocaml-lsp-server": "*"
  }
}
